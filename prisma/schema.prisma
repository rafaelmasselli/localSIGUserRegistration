generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String   @id @unique @default(cuid())
  fullName      String
  birthDate     DateTime @map(name: "birthDate")
  cpf           String   @unique
  email         String   @unique
  phone         String
  maritalStatus String
  address       String
  cep           String
  age           Int
}
